#include <iostream>

using namespace std;

int main () 
{
    int a, b, c;
    cout << "Ingrese el primer numero: ";
    cin >> a;
    cout << "Ingrese el segundo numero: ";
    cin >> b;
    c = a + b;
    cout << "La suma es: " << c << endl;
    c = a - b;
    cout << "La resta es: " << c << endl;
    c = a * b;
    cout << "La multiplicaion es: " << c << endl;
    c = a / b;
    cout << "La division es: " << c << endl;
}
